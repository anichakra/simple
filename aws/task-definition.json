{
    "family": "simple-rest-service-task",
    "containerDefinitions": [
        {
            "image": "%ACCOUNT%.dkr.ecr.us-east-1.amazonaws.com/simple-rest-service:%BUILD_TAG%",
            "name": "simple-rest-service-task",
            "essential": true,
            "cpu": 100,
            "memoryReservation": 2048,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 9443,
                    "hostPort": 9443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}